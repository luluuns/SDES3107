//Use multiplication to create a series of lines with increasing space between each.

int x = 10;


size(500,500);
background(102,202,302);
strokeWeight(2);
line(10,10,150,10);
line(x * 2,20,150,x * 2);
line(x * 3,30,150,x * 3);
line(x * 4,40,150,x * 4);
line(x * 5,50,150,x * 5);
line(x * 6,60,150,x * 6);
line(x * 7,70,150,x * 7);
